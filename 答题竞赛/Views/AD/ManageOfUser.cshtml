@model IQueryable<答题竞赛.Models.User>
@{
    ViewBag.Title = "ManageOfUser";
    Layout = "~/Views/Shared/_ADshareview.cshtml";
}

<div class="card">
    <div class="card-header c_text">
        <h4>账户管理</h4>
    </div>
    <div class="card-body c_text">
        <table class="table table-striped table-hover">
            <thead>
            <th>编号</th>
            <th>账号</th>
            <th>加入时间</th>
            <th>操作</th>
            </thead>
@foreach (var item in Model)
{
            <tr>
                <td>@item.User_id</td>
                <td>@item.User_Name</td>
                <td>@item.Add_Time</td>
                <td>@Html.ActionLink("操作", "Userinfor", new { uid=item.User_id})</td>
            </tr>
        
}
        </table>
    </div>
</div>
